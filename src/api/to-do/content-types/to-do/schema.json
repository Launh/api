{
  "kind": "collectionType",
  "collectionName": "to_dos",
  "info": {
    "singularName": "to-do",
    "pluralName": "to-dos",
    "displayName": "ToDo",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "date": {
      "type": "datetime"
    },
    "pet": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::pet.pet"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "food_purchase",
        "deworming",
        "deflea",
        "vaccination",
        "grooming",
        "water_fountain_cleaning",
        "cat_litter_purchase",
        "cat_litter_refill",
        "custom_reminder"
      ]
    },
    "interval_type": {
      "type": "enumeration",
      "enum": [
        "d",
        "m",
        "y"
      ],
      "default": "d"
    },
    "interval_value": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "reminder": {
      "type": "boolean",
      "default": false
    },
    "note": {
      "type": "text"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "private": true
    }
  }
}
