{
  "kind": "collectionType",
  "collectionName": "pets",
  "info": {
    "singularName": "pet",
    "pluralName": "pets",
    "displayName": "Pet",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "minLength": 1
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "sex": {
      "type": "enumeration",
      "enum": [
        "male",
        "female"
      ]
    },
    "breed": {
      "type": "string"
    },
    "dob": {
      "type": "date"
    },
    "chipId": {
      "type": "string"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
